"UnlitGeneric"
{
	"$translucent" 1
	"$basetexture" "vgui\replay\thumbnails\tf2.vtf"
	"$yertexcolor" 1
	"$no_fullbright" 1
	"$ignorez" 1

	"$angle" "0.0"
	"$translate" "[0.0 0.0]"
	"$center" "[0.5 0.5]"

	"Proxies"
	{

		// Rotates the logo
		"LinearRamp"
		{
			"rate" "-5" // Adjust this, higher spins faster(negative clockwise, positive counter-clockwise)
			"initialValue" "0.0"
			"resultVar" "$angle"
		}
		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"centerVar" "$center"
			"resultVar" "$basetexturetransform"
		}
	}

}
